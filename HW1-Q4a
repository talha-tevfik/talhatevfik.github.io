#include <stdio.h>

int computeHomeValue(int popularity, int size)
{
    return (popularity * popularity * popularity + size * size) * 10000;
}

int main(void)
{
    int popularity, size;
    int homeValue; 

    printf("Enter popularity: ");
    scanf("%d", &popularity);

    printf("Enter size: ");
    scanf("%d", &size);

    homeValue = computeHomeValue(popularity, size);

    printf("Home value is: %d\n", homeValue);

    return 0; 
}
